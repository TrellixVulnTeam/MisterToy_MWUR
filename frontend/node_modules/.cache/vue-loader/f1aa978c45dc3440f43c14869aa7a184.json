  ></el-input>\r\n    </label>\r\n    <label v-if=\"!loggedinUser\"\r\n      >Password:\r\n      <el-input\r\n        id=\"password\"\r\n        class=\"input\"\r\n        type=\"password\"\r\n        placeholder=\"Password\"\r\n        v-model=\"user.password\"\r\n      ></el-input>\r\n    </label>\r\n    <div class=\"buttons\">\r\n      <button class=\"btn\" @click=\"login\" v-if=\"!loggedinUser\">Login</button>\r\n      <button class=\"btn\" @click=\"clickSignup\" v-if=\"!loggedinUser\">\r\n        Singup\r\n      </button>\r\n      <button class=\"btn\" @click=\"logout\" v-else>Logout</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport signup from \"./signup.vue\";\r\nexport default {\r\n  components: { signup },\r\n  data() {\r\n    return {\r\n      title: null,\r\n      user: {\r\n         username:'',\r\n         password:''\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        await this.$store.dispatch({ type: \"login\", userCred: this.user });\r\n        this.user = {};\r\n      } catch (err) {\r\n        console.log(\"cannot login\", err);\r\n        throw err;\r\n      }\r\n    },\r\n    async logout() {\r\n      try {\r\n        await this.$store.dispatch({ type: \"logout\" });\r\n      } catch (err) {\r\n        console.log(\"cannot logout\", err);\r\n        throw err;\r\n      }\r\n    },\r\n    clickSignup() {\r\n      this.$emit(\"signup\", true);\r\n    },\r\n    async signup() {\r\n      try {\r\n        await this.$store.dispatch({ type: \"signup\" });\r\n      } catch (err) {\r\n        console.log(\"cannot signup\", err);\r\n        throw err;\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    loggedinUser() {\r\n      this.title = this.$store.getters.loggedinUser ? \"Logout\" : \"Login\";\r\n      return this.$store.getters.loggedinUser;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}