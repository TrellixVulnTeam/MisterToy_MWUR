,MAAA,OAFA,qBAEA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,KADA,GACA,KAAA,CAAA,MAAA,CAAA,MADA,CACA,KADA;AAAA;AAAA;AAAA,yBAGA,UAAA,CAAA,OAAA,CAAA,KAAA,CAHA;;AAAA;AAGA,kBAAA,GAHA;AAIA,kBAAA,KAAA,CAAA,GAAA,GAAA,GAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,OAAA,yBAAA,KAAA,CAAA,GAAA,CAAA,MAAA,UAAA;AALA;AAAA,yBAMA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA;AAAA,oBAAA,IAAA,EAAA,aAAA;AAAA,oBAAA,QAAA,EAAA;AAAA,+BAAA;AAAA;AAAA,mBAAA,CANA;;AAAA;AAMA,kBAAA,KAAA,CAAA,OANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAQA,kBAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AARA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAbA;AADA,GATA;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,MADA,EACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA,oBAEA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,MAFA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAGA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,UAAA,CAHA;;AAAA;AAIA,gBAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,GAAA,CAAA,GAAA;AAJA;AAAA,uBAKA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA;AAAA,kBAAA,IAAA,EAAA,WAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,iBAAA,CALA;;AAAA;AAAA;AAAA,uBAMA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA;AAAA,kBAAA,IAAA,EAAA,aAAA;AAAA,kBAAA,QAAA,EAAA;AAAA,6BAAA,MAAA,CAAA,GAAA,CAAA;AAAA;AAAA,iBAAA,CANA;;AAAA;AAMA,gBAAA,MAAA,CAAA,OANA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAQA,gBAAA,OAAA,CAAA,GAAA,CAAA,mBAAA;AARA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAZA,GA1BA;AAwCA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,QAAA,EAAA;AAFA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"toy-details\" v-if=\"toy\">\r\n   <el-button type=\"primary\" class=\"open-chat-btn\" @click=\"isChatOpen = true\"\r\n    v-if=\"!isChatOpen\">Open Chat</el-button>\r\n    <chat-room v-else @closeChat=\"isChatOpen = false\"/>\r\n    \r\n    <img class=\"toy-img\" :src=\"imgUrl\" v-if=\"imgUrl\" />\r\n    <review :reviews=\"reviews\" @addReview=\"addReview\" />\r\n    <section class=\"info1\">\r\n      <p>Id: {{ toy._id }}</p>\r\n      <p>Name: {{ toy.name }}</p>\r\n      <p>Price: {{ toy.price }}</p>\r\n    </section>\r\n    <section class=\"info2\">\r\n      <p>Type: {{ toy.type }}</p>\r\n      <p>\r\n        Created at:\r\n        <span>{{ toy.createdAt | moment(\"ddd, MMMM  YYYY, ha\") }}</span>\r\n      </p>\r\n      <p>in stock: {{ toy.inStock }}</p>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { toyService } from \"../services/toy.service\";\r\nimport review from \"../cmps/review.vue\";\r\nimport chatRoom from \"../cmps/chat-room.vue\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      toy: null,\r\n      imgUrl: null,\r\n      reviews: null,\r\n      isChatOpen:false\r\n    };\r\n  },\r\n  watch: {\r\n    \"$route.params.toyId\": {\r\n      immediate: true,\r\n      async handler() {\r\n        const { toyId } = this.$route.params;\r\n        try {\r\n          const toy = await toyService.getById(toyId);\r\n          this.toy = toy;\r\n          this.imgUrl = require(`@/assets/toys/${this.toy.imgNum}.png`);\r\n          this.reviews = await this.$store.dispatch({ type: \"loadReviews\", filterBy:{'toyId':toyId }});\r\n        } catch (err) {\r\n          console.log(\"cannot get toy\", err);\r\n          throw err;\r\n        }\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    async addReview(review) {\r\n      try {\r\n        if (!this.$store.getters.toys.length)\r\n         await this.$store.dispatch(\"loadToys\");\r\n        review.toyId = this.toy._id;\r\n        await this.$store.dispatch({ type: \"addReview\", review });\r\n         this.reviews = await this.$store.dispatch({ type: \"loadReviews\" ,filterBy:{'toyId':this.toy._id }});\r\n      } catch (err) {\r\n        console.log(\"cannot add review\", err);\r\n        throw err;\r\n      }\r\n    },\r\n  },\r\n  components: {\r\n    review,\r\n    chatRoom\r\n  },\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/pages"}]}